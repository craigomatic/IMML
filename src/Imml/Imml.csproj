<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Implementation of the Immersive Media Markup Language (IMML) that makes it simple to read markup into an object model or write an object model for transmission or storage.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="imml.xsd" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="imml.xsd" />
  </ItemGroup>

</Project>
